@using DigitalLearningSolutions.Web.ViewModels.LearningSolutions
@model ErrorViewModel
@{
  ViewData["Title"] = "Digital Learning Solutions - Page not found";
}

@section NavMenuItems {
  <partial name="_NavMenuItems" />
}

<div class="nhsuk-grid-row">
  <div class="nhsuk-grid-column-two-thirds nhsuk-u-reading-width">
    <h1>We can’t find the page you’re looking for</h1>
    <p>Please check the web address to make sure it was correct.</p>
    <p>Or return to the <a asp-controller="Home" asp-action="Index">Digital Learning Solutions homepage</a>.</p>
    <p>@Model.HelpText()</p>
  </div>
</div>
