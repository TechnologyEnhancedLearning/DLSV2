@using DigitalLearningSolutions.Web.Models;
@using DigitalLearningSolutions.Web.ViewModels.LearningSolutions
@model AccessibilityHelpViewModel
@{
  ViewData["Title"] = "Digital Learning Solutions - Accessibility";
  ViewData["DoNotDisplayNavBar"] = true;
}
<link rel="stylesheet" type="text/css" href="~/css/home/Policies.css" asp-append-version="true">

<div class="nhsuk-grid-row">
  <div class="nhsuk-grid-column-full">
        @Html.Raw(Model.AccessibilityHelpText)
        @await Html.PartialAsync("~/Views/Shared/_PageReviewPartial.cshtml", new PageReviewModel { LastReviewedDate = Model.LastReviewedDate, NextReviewDate = Model.NextReviewDate })
  </div>
</div>
