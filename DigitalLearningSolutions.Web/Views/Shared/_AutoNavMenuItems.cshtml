@using DigitalLearningSolutions.Data.Enums
@using DigitalLearningSolutions.Web.Models.Enums
@model ApplicationType

@if (ApplicationType.TrackingSystem.Equals(Model)) {
  <partial name="../TrackingSystem/Shared/_NavMenuItems" />
} else if (ApplicationType.LearningPortal.Equals(Model)) {
  <partial name="../LearningPortal/Shared/_NavMenuItems" />
} else if (ApplicationType.Frameworks.Equals(Model)) {
  <partial name="../Frameworks/Shared/_NavMenuItems" />
} else if (ApplicationType.Supervisor.Equals(Model)) {
  <partial name="../Supervisor/Shared/_NavMenuItems" />
} else {
  <vc:nav-menu-and-view-data application="@Model" selected-tab="@Tab.MyAccount" />
}
