@using DigitalLearningSolutions.Web.Models.Enums
@using DigitalLearningSolutions.Web.ViewModels.Common.ViewComponents
@model NavMenuAndViewDataViewModel

@{
  ViewData["Application"] = Model.Application.Name;
  ViewData["HeaderPath"] = Model.Application.HeaderPath;
  ViewData["HeaderPathName"] = Model.Application.HeaderPathName;
}

@if (ApplicationType.TrackingSystem.Equals(Model.Application)) {
  <partial name="../TrackingSystem/Shared/_NavMenuItems" model="@Model.SelectedTab" />
} else if (ApplicationType.LearningPortal.Equals(Model.Application)) {
  <partial name="../LearningPortal/Shared/_NavMenuItems" model="@Model.SelectedTab" />
} else if (ApplicationType.Frameworks.Equals(Model.Application)) {
  <partial name="../Frameworks/Shared/_NavMenuItems" model="@Model.SelectedTab" />
} else if (ApplicationType.Supervisor.Equals(Model.Application)) {
  <partial name="../Supervisor/Shared/_NavMenuItems" model="@Model.SelectedTab" />
} else {
  <partial name="_NavMenuItems" model="@Model.SelectedTab" />
}
