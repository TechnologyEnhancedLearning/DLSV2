@{
  ViewData["Title"] = "Multiple users have the same email address";
}

<div class="nhsuk-grid-row">
  <div class="nhsuk-grid-column-full">
    <h1 class="nhsuk-heading-xl">@ViewData["Title"]</h1>
    <p class="nhsuk-body-l">
      Multiple users have the same email address, so it is not possible to determine which user to reset the password for. For assistance with this issue, please contact <a href="mailto:@ViewData["SupportEmail"]">@ViewData["SupportEmail"]</a>.
    </p>
  </div>
</div>
