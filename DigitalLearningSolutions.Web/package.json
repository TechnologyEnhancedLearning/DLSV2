    "js-cookie": "^3.0.5",
    "js-search": "^2.0.1",
    "lodash": "^4.17.20",
    "nhsuk-frontend": "^6.2.0",
    "regenerator-runtime": "^0.14.1"
  },
  "devDependencies": {
    "@babel/core": "^7.23.6",
    "@babel/preset-env": "^7.23.9",
    "@babel/preset-typescript": "^7.23.3",
    "@types/dompurify": "^2.3.3",
    "@types/jest": "^27.5.2",
