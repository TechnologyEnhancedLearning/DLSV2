{
  "version": "1.0.0",
  "name": "digital-learning-solutions",
  "private": true,
  "engines": {
    "node": ">=12.0.0",
    "yarn": ">=1.22.0"
  },
  "scripts": {
    "prebuild": "yarn clean",
    "build": "run-p build:*",
    "build:sass": "yarn sass --quiet",
    "build:webpack": "webpack",
    "lint": "eslint ./Scripts/**/*.ts",
    "lint-fix": "eslint --fix ./Scripts/**/*.ts",
    "dev": "run-p dev:*",
    "dev:sass": "yarn sass --watch --quiet",
    "dev:webpack": "webpack --config webpack.config.dev.js",
    "clean": "rimraf wwwroot/css/ && rimraf wwwroot/js/",
    "sass": "sass Styles:wwwroot/css --load-path=node_modules --quiet-deps --style=compressed",
    "test": "jest"
  },
  "dependencies": {
    "@types/chartist": "^0.11.1",
    "chartist": "^0.11.4",
    "core-js": "^3.22.5",
    "input-range-scss": "^1.5.2",
    "jodit": "^3.18.5",
    "js-cookie": "^3.0.1",
    "js-search": "^2.0.0",
    "lodash": "^4.17.20",
    "moment": "^2.29.3",
    "nhsuk-frontend": "^6.1.0",
    "regenerator-runtime": "^0.13.9"
  },
  "devDependencies": {
    "@babel/core": "^7.18.0",
    "@babel/preset-env": "^7.18.0",
    "@babel/preset-typescript": "^7.17.12",
    "@types/jest": "^27.5.1",
    "@types/js-cookie": "^3.0.2",
    "@types/js-search": "^1.4.0",
    "@types/jsdom": "^16.2.14",
    "@types/lodash": "^4.14.182",
    "@typescript-eslint/eslint-plugin": "^5.25.0",
    "@typescript-eslint/parser": "^5.25.0",
    "babel-jest": "^28.1.0",
    "babel-loader": "^8.2.5",
    "eslint": "^8.15.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-import-resolver-webpack": "^0.13.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jasmine": "^4.1.3",
    "fork-ts-checker-webpack-plugin": "^7.2.11",
    "glob": "^8.0.3",
    "jest": "^28.1.0",
    "jest-environment-jsdom": "^28.1.0",
    "jsdom": "^19.0.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.4",
    "webpack": "^5.72.1",
    "webpack-cli": "^4.9.2"
