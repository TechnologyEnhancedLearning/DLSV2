{
  "version": "1.0.0",
  "name": "digital-learning-solutions",
  "private": true,
  "scripts": {
    "prebuild": "npm run clean",
    "build": "run-p build:*",
    "build:sass": "npm run sass -- --output-style compressed",
    "build:webpack": "webpack",
    "lint": "eslint ./Scripts/**/*.ts",
    "lint-fix": "eslint --fix ./Scripts/**/*.ts",
    "dev": "run-p dev:*",
    "dev:sass": "npm run sass && npm run sass -- --watch",
    "dev:webpack": "webpack --config webpack.config.dev.js",
    "clean": "rimraf wwwroot/css/ && rimraf wwwroot/js/",
    "sass": "node-sass Styles/ --importer node_modules/node-sass-package-importer/dist/cli.js --output wwwroot/css/",
    "test": "jest"
  },
  "dependencies": {
    "core-js": "^3.7.0",
    "input-range-scss": "^1.5.2",
    "js-search": "^2.0.0",
    "lodash": "^4.17.20",
    "moment": "^2.29.1",
    "nhsuk-frontend": "4.0.0",
    "regenerator-runtime": "^0.13.7"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-typescript": "^7.12.1",
    "@types/jest": "^26.0.15",
    "@types/js-search": "^1.4.0",
    "@types/jsdom": "^16.2.5",
    "@types/lodash": "^4.14.165",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.1",
    "copy-webpack-plugin": "^6.3.1",
    "eslint": "^7.13.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-import-resolver-webpack": "^0.12.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jasmine": "^4.1.1",
    "fork-ts-checker-webpack-plugin": "^5.2.1",
    "glob": "7.0.4",
    "jest": "^26.6.3",
    "jsdom": "^16.4.0",
    "node-sass": "4.14.1",
    "node-sass-package-importer": "5.3.2",
    "npm-run-all": "4.1.5",
    "rimraf": "3.0.2",
    "source-map-loader": "0.2.4",
    "ts-loader": "7.0.4",
    "ts-node": "^8.10.2",
    "typescript": "3.9.2",
    "webpack": "4.43.0",
    "webpack-cli": "3.3.11"
  },
  "-vs-binding": {
    "ProjectOpened": [
      "dev"
    ]
  }
}
